<script type="text/javascript">
	$("input:checkbox").change(function(){
 var group = ":checkbox[name='"+ $(this).attr("name") + "']";
   if($(this).is(':checked')){
     $(group).not($(this)).attr("checked",false);
   }
    if($(this).is(':checked')){
   	var links = "https://test-prod-api.herokuapp.com/products?cat="+$(this).val()
   }
   else{
   	var links = "https://test-prod-api.herokuapp.com/products"
   }
   $( "#content div p" ).remove();
   $( "#page_navigation" ).remove();
   dataLoad(links)
 });
</script>